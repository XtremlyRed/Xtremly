<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Label="Globals">
		<SccProjectName>SAK</SccProjectName>
		<SccProvider>SAK</SccProvider>
		<SccAuxPath>SAK</SccAuxPath>
		<SccLocalPath>SAK</SccLocalPath>
		<Platforms>AnyCPU</Platforms>
	</PropertyGroup>



	<PropertyGroup>
		<Copyright>©ismyhy</Copyright>
		<Version>5.2</Version>
		<AssemblyVersion>5.2</AssemblyVersion>
		<LangVersion>10.0</LangVersion>
		<PackageProjectUrl>https://github.com/XtremlyRed/Xtremly</PackageProjectUrl>
	</PropertyGroup>



	<PropertyGroup>
		<OutputPath>$(SolutionDir)\src\..\bin\$(Platform)\$(Configuration)</OutputPath>
	 
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>

	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Release'">

		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>

	</PropertyGroup>

	<!--使用方式-->
	<!--把需要同步生成路径的项目卸载，编辑项目文件，在项目文件底部添加下面代码段-->
	<!--<Import Project="$(SolutionDir)\GlobalCompileSettings.Targets" />-->

</Project>